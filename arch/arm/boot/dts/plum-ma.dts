	aliases {
		xio_02 = &xio_02;
		xio_03 = &xio_03;
		xio_04 = &xio_04;

		xio_06 = &xio_06;

		xio_07 = &xio_07_0a;
		xio_08 = &xio_07_0a;
		xio_09 = &xio_07_0a;
		xio_0a = &xio_07_0a;

		xio_0b = &xio_0b;
		xio_0c = &xio_0c;

		xio_0d = &xio_0d_0e;
		xio_0e = &xio_0d_0e;

		xio_0f = &xio_0f_10;
		xio_10 = &xio_0f_10;

		xio_12 = &xio_12;
		xio_13 = &xio_13_14;
		xio_14 = &xio_13_14;
		xio_15 = &xio_15;
		xio_16 = &xio_16_17;
		xio_17 = &xio_16_17;
	};

	/* XIO: (id: 0x02) FOMA only */
	xio_02: xio@0x02 {
		#size-cells = <0>;
		devices = <&gpiox_0x10 &gpiox_0x12 &gpiox_0x14
			   &leds_foma &leds_foma_ctrl>;
	};

	/* XIO: (id: 0x03) FOMA/UARTx2/DIO */
