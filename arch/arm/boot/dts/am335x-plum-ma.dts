/include/ "tps65217.dtsi"

&tps {
	interrupts = <7>; /* NMI */
	interrupt-parent = <&intc>;

	/*ti,pmic-shutdown-controller;*/

	regulators {
		dcdc1_reg: regulator@0 {
			regulator-name = "vdds_dpr";
			regulator-always-on;
		};

		dcdc2_reg: regulator@1 {
			/* VDD_MPU voltage limits 0.95V - 1.26V with +/-4% tolerance */
			regulator-name = "vdd_mpu";
			regulator-min-microvolt = <925000>;
			regulator-max-microvolt = <1351500>;
			regulator-boot-on;
			regulator-always-on;
		};

		dcdc3_reg: regulator@2 {
			/* VDD_CORE voltage limits 0.95V - 1.1V with +/-4% tolerance */
			regulator-name = "vdd_core";
			regulator-min-microvolt = <925000>;
			regulator-max-microvolt = <1150000>;
			regulator-boot-on;
			regulator-always-on;
		};

		ldo1_reg: regulator@3 {
			regulator-name = "vio,vrtc,vdds";
			regulator-always-on;
		};

		ldo2_reg: regulator@4 {
			regulator-name = "vdd_3v3aux";
			regulator-always-on;
		};

		ldo3_reg: regulator@5 {
			regulator-name = "vdd_1v8";
			regulator-always-on;
		};

		ldo4_reg: regulator@6 {
			regulator-name = "vdd_3v3a";
			regulator-always-on;
		};
	};
};

&mac {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&cpsw_default>;
	pinctrl-1 = <&cpsw_sleep>;
	mac_control = <0x38>;
	dual_emac = <1>;

	no_bd_ram = <0>;
	rx_descs = <128>;
	status = "okay";
};

&davinci_mdio {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&davinci_mdio_default>;
	pinctrl-1 = <&davinci_mdio_sleep>;
	status = "okay";

	phy0-reset-gpio = <&gpio3 18 GPIO_ACTIVE_LOW>;
	phy1-reset-gpio = <&gpio3 19 GPIO_ACTIVE_LOW>;

	ethphy0: ethernet-phy@0 {
		reg = <2>;

		#interrupt-cells = <2>;
		interrupt-parent = <&gpio0>;
		interrupts = <6 8>;
		irq-gpio = <&gpio0 6 GPIO_ACTIVE_LOW>;

		rxc-skew-ps = <1020>;
		txc-skew-ps = <1140>;
		rxdv-skew-ps = <420>;
		txen-skew-ps = <420>;
		rxd0-skew-ps = <420>;
		rxd1-skew-ps = <420>;
		rxd2-skew-ps = <420>;
		rxd3-skew-ps = <420>;
		txd0-skew-ps = <420>;
		txd1-skew-ps = <420>;
		txd2-skew-ps = <420>;
		txd3-skew-ps = <420>;
	};

	ethphy1: ethernet-phy@1 {
		reg = <3>;

		#interrupt-cells = <2>;
		interrupt-parent = <&gpio0>;
		interrupts = <20 8>;
		irq-gpio = <&gpio0 20 GPIO_ACTIVE_LOW>;

		rxc-skew-ps = <1140>;
		txc-skew-ps = <1440>;
		rxdv-skew-ps = <420>;
		txen-skew-ps = <420>;
		rxd0-skew-ps = <420>;
		rxd1-skew-ps = <420>;
		rxd2-skew-ps = <420>;
		rxd3-skew-ps = <420>;
		txd0-skew-ps = <420>;
		txd1-skew-ps = <420>;
		txd2-skew-ps = <420>;
		txd3-skew-ps = <420>;
	};
};

&cpsw_emac0 {
	phy-handle = <&ethphy0>;

	phy-mode = "rgmii-txid";
	dual_emac_res_vlan = <4094>;
};

&cpsw_emac1 {
	phy-handle = <&ethphy1>;

	phy-mode = "rgmii-txid";
	dual_emac_res_vlan = <4095>;
};

&mmc1 {
	status = "okay";
	vmmc-supply = <&vmmcsd_fixed>;
	bus-width = <4>;
	pinctrl-names = "default";
	pinctrl-0 = <&mmc1_pins>;
	cd-gpios = <&gpio3 0 GPIO_ACTIVE_LOW>;
	wp-gpios = <&gpio3 17 GPIO_ACTIVE_HIGH>;
};

&spi0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&spi0_pins>;
	ti,spi-num-cs = <1>;

	/* spi0.0: Micron 16Mbit NOR Flash (M25P16) */
	spi_nor: flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "m25p16";
		spi-max-frequency = <20000000>;
		m25p,fast-read;
		reg = <0>;

		/* MTD partition table */
		partition@0 {
			label = "MLO";
			reg = <0x00000000 0x00020000>;
		};

		partition@1 {
			label = "barebox.bin";
			reg = <0x00020000 0x00080000>;
		};

		partition@2 {
			label = "barebox.env";
			reg = <0x000a0000 0x00020000>;
		};

		partition@3 {
			label = "dtb.0";
			reg = <0x000c0000 0x00020000>;
		};

		partition@4 {
			label = "dtb.1";
			reg = <0x000e0000 0x00020000>;
		};

		partition@5 {
			label = "hist_firmup";
			reg = <0x00100000 0x00020000>;
		};

		partition@6 {
			label = "hist_boot";
			reg = <0x00120000 0x000e0000>;
		};
	};
};

&spi1 {
	ti,spi-num-cs = <2>;

	pinctrl-names = "default";
	pinctrl-0 = <&spi1_pins>;

	ltc1859_0: ltc1859@0 {
		compatible = "ltc185x";
		reg = <0>;
		spi-max-frequency = <1000000>;

		vcc-supply = <&xio_adc_supply>;
	};

	ltc1859_1: ltc1859@1 {
		compatible = "ltc185x";
		reg = <1>;
		spi-max-frequency = <1000000>;

		vcc-supply = <&xio_adc_supply>;
		status = "disabled";
	};
};

&rtc {
	system-power-controller;
};

&gpio0 {
	wakeup-source;
};

&gpu_target {
	status = "disabled";
};

&tscadc {
	status = "okay";
	adc {
		ti,adc-channels = <0 1>;
	};
};
