&mcu_uart0 {
	status = "disabled";
};

&mcu_uart1 {
	status = "disabled";
};

&main_uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&main_uart0_pins_default>;
	dtr-gpios = <&main_gpio0 27 GPIO_ACTIVE_LOW>;
	dsr-gpios = <&main_gpio0 28 GPIO_ACTIVE_LOW>;
	dcd-gpios = <&main_gpio0 29 GPIO_ACTIVE_LOW>;
	rng-gpios = <&main_gpio0 30 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&main_uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&main_uart1_pins_default>;
	status = "okay";
};

&main_uart2 {
	status = "disabled";
};

&main_uart3 {
	status = "disabled";
};

&main_uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&main_uart4_pins_default>;
	linux,rs485-enabled-at-boot-time;
	rs485-rts-active-low;
	status = "okay";
};

&main_uart5 {
	status = "disabled";
};

&main_uart6 {
	status = "disabled";
};

&mcu_i2c0 {
	status = "disabled";
};

&mcu_i2c1 {
	status = "disabled";
};

&main_i2c0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&main_i2c0_pins_default>;
	clock-frequency = <400000>;
};

&main_i2c1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&main_i2c1_pins_default>;
	clock-frequency = <400000>;

	dsk324sr: rtc@32 {
		compatible = "dsk324sr";
		reg = <0x32>;
	};

	tca6507@45 {
		compatible = "ti,tca6507";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x45>;

		led0: power-status-red@0 {
			label = "power-stat:r";
			reg = <0x0>;
		};

		led1: system-status1-red@1 {
			label = "system-status1:r";
			reg = <0x1>;
			linux,default-trigger = "heartbeat";
		};

		led2: system-status2-red@2 {
			label = "system-status2:r";
			reg = <0x2>;
		};

		led3: system-status3-red@3 {
			label = "system-status3:r";
			reg = <0x3>;
		};

		led4: system-status1-green@4 {
			label = "system-status1:g";
			reg = <0x4>;
			linux,default-trigger = "timer-suspend";
		};

		led5: system-status2-green@5 {
			label = "system-status2:g";
			reg = <0x5>;
		};

		led6: system-status3-green@6 {
			label = "system-status3:g";
			reg = <0x6>;
		};
	};

	temp_senser: tmp75b@4a {
		compatible = "ti,tmp75b";
		reg = <0x4a>;
	};

	io_exp: io_exp@4d {
		compatible = "nxp,pca9698";
		reg = <0x4d>;
		#address-cells = <1>;
		#size-cells = <0>;
		#gpio-cells = <2>;

		gpio-line-names = "Mobile_Power_MSP", "EXT_Mobile_Power_MSP", "",
				  "", "", "", "Has_PCIe", "SIM_Select",
				  "", "", "", "", "", "", "", "", /* Reserved */
				  "", "", "", "", "", "", "", "", /* Reserved */
				  "", "", "", "", "", "", "", "", /* Reserved */
				  "", "", "", "", "", "", "", ""; /* Reserved */
	};

	i2c1_eeprom: eeprom@50 {
		compatible = "atmel,24c02";
		reg = <0x50>;
	};

	m41t80: rtc@68 {
		compatible = "st,m41t80";
		reg = <0x68>;
		wakeup-source;
	};
};

